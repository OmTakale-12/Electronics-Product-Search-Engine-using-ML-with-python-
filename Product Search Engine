    "from nltk.stem import PorterStemmer\n",
    "\n",
    "def stemming(text):\n",
    "    stemmer = PorterStemmer()  # Initialize the stemmer\n",
    "    words = []  # Initialize a list to hold the stemmed words\n",
    "    for word in text.split(' '):  # Split the text by spaces\n",
    "        words.append(stemmer.stem(word))  # Stem each word and append to the list\n",
    "    return ' '.join(words)  # Join the stemmed words back into a single string\n",
    "\n",
    "# Example usage\n",
    "text = \"Dancer dancing in the dance floor. While dancing he fell on the floor and dancer's dance stopped.\"\n",
    "stemming(text)\n",
    "\n"
   ]
  },
  {
