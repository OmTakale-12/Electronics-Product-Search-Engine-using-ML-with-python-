  {
   "cell_type": "code",
   "execution_count": 92,
   "id": "fa5c68bb-e64f-4e09-a7b5-b7a74b100dad",
   "metadata": {},
   "outputs": [],
   "source": [
    "def recommender(product, similarity):\n",
    "    product_index = electronics[electronics['name'] == product].index[0]\n",
    "    similarity_list = list(enumerate(similarity[product_index]))\n",
    "    top_10_similar_product = sorted(similarity_list, key=lambda x: x[1], reverse=True)[1:11]\n",
    "    for idx, sim in top_10_similar_product:\n",
    "        print(idx)\n",
    "        print(electronics.loc[idx]['name'])\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 93,
   "id": "2d8011f6-f069-4af7-bcec-8d7b884ee7b4",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
